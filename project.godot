config_version=5

[application]

config/name="Splash!"
config/version="0.3.2 WIP"
run/main_scene="res://Scenes/start_window.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=30
config/icon="res://icon.svg"

[autoload]

settings="*res://Scripts/varables.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"Player": "res://DG_resourse/Characters/Player.dch",
"npc1": "res://DG_resourse/Characters/npc1.dch",
"npc2": "res://DG_resourse/Characters/npc2.dch",
"npc3": "res://DG_resourse/Characters/npc3.dch"
}
directories/dtl_directory={
"tl_1": "res://DG_resourse/Timelines/tl_1.dtl",
"tl_2": "res://DG_resourse/Timelines/tl_2.dtl",
"tl_3": "res://DG_resourse/Timelines/tl_3.dtl"
}
layout/style_list=["res://DG_resourse/Styles/Default.tres"]
